    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:layout_below="@+id/my_uplayout"
        android:layout_marginTop="15dp"
        android:orientation="vertical"
        >

        <LinearLayout
            android:id="@+id/my_receipt_layout"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="18sp"
                android:fontFamily="@font/nanumsquareotfregular"
                android:text="예약내역"
                />
        </LinearLayout>

        // 1 계정
        <LinearLayout
            android:id="@+id/my_account_layout"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="18sp"
                android:fontFamily="@font/nanumsquareotfregular"
                android:text="계정"
                />
        </LinearLayout>
        // 2 푸쉬알림
        <LinearLayout
            android:id="@+id/my_pushalram_layout"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="18sp"
                android:fontFamily="@font/nanumsquareotfregular"
                android:text="푸쉬알림"
                />
        </LinearLayout>
        // 3 고객센터
        <LinearLayout
            android:id="@+id/my_customer_center_layout"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="18sp"
                android:fontFamily="@font/nanumsquareotfregular"
                android:text="고객센터"
                />
        </LinearLayout>
        // 4 로그아웃
        <LinearLayout
            android:id="@+id/my_logout_layout"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="18sp"
                android:fontFamily="@font/nanumsquareotfregular"
                android:text="로그아웃"
                />
        </LinearLayout>

    </LinearLayout>




    <RelativeLayout
        android:id="@+id/main_bottom"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="bottom"
        >

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_light_bolder"
            android:layout_alignParentTop="true"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            >

            <LinearLayout
                android:id="@+id/my_rescompletelayout"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    >
                    <ImageView
                        android:id="@+id/main_reservation_imageview"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="center"
                        android:background="@drawable/ic_icon_call"
                        android:backgroundTint="@color/color_bolder"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"/>
                    <TextView
                        android:id="@+id/main_reservation_txtview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="45dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/nanumsquareotfregular"
                        android:layout_centerVertical="true"
                        android:text="예약 완료"
                        android:textColor="@color/color_bolder"
                        android:textSize="16sp" />

                    <View
                        android:layout_alignParentRight="true"
                        android:layout_width="1dp"
                        android:layout_height="60dp"
                        android:background="@color/color_light_bolder"
                        />

                </RelativeLayout>




            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_waitmatchinglayout"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_marginLeft="15dp"
                        android:id="@+id/main_wait_matching_imageview"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_onwait_purple"
                        android:backgroundTint="@color/color_bolder" />
                    <TextView
                        android:id="@+id/main_wait_matching_txtview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="45dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/nanumsquareotfregular"
                        android:layout_centerVertical="true"
                        android:text="매칭 대기"
                        android:textColor="@color/color_bolder"
                        android:textSize="16sp" />
                    <View
                        android:layout_alignParentRight="true"
                        android:layout_width="1dp"
                        android:layout_height="60dp"
                        android:background="@color/color_light_bolder"
                        />
                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_mymenulayout"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:id="@+id/onwait_mymenulayout"
                    android:layout_width="fill_parent"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        >
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true">
                            <ImageView
                                android:id="@+id/main_my_imageview"
                                android:layout_width="19dp"
                                android:layout_height="20dp"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="8dp"
                                android:background="@drawable/ic_icon_shop"
                                android:backgroundTint="@color/color_bolder" />
                            <TextView
                                android:id="@+id/main_my_txtview"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/nanumsquareotfregular"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/main_my_imageview"
                                android:text="My"
                                android:textColor="@color/color_bolder"
                                android:textSize="15sp" />
                        </LinearLayout>
                    </RelativeLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>


    </RelativeLayout>

</RelativeLayout>